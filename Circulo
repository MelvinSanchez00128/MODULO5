package proyecformasmelvin;

/**

 */
public class Circulo {
      private double radio;

    public Circulo(double radio) {
        this.radio = radio;
    }

    public void calcularRadio() {
        System.out.println("Radio: " + radio);
    }

    public void dibujar() {
        System.out.println("Circulo.");
    }
}
