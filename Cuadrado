package proyecformasmelvin;

/**

 */
public class Cuadrado {
      private double area;

    public Cuadrado(double area) {
        this.area = area;
    }

    public void calcularArea() {
        System.out.println("Area del cuadrado: " + area);
    }

    public void dibujar() {
        System.out.println("Cuadrado.");
    }
}
