package proyecformasmelvin;

/**

 */
public class ProyecFormasMelvin {

   
    public static void main(String[] args) {
        Circulo circulo = new Circulo(15.0);
        Linea linea = new Linea(500.0);
        Triangulo triangulo = new Triangulo(30.0);
        Cuadrado cuadrado = new Cuadrado(38.0);

        circulo.dibujar();
        circulo.calcularRadio();

        linea.dibujar();

        triangulo.dibujar();
        triangulo.calcularArea();

        cuadrado.dibujar();
        cuadrado.calcularArea();
    }
    
}
